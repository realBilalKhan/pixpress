{
  "name": "pixpress",
  "version": "1.0.0",
  "description": "A powerful CLI tool for image processing with HEIC/HEIF support, batch processing, meme creation, and more",
  "main": "index.js",
  "bin": {
    "pixpress": "./index.js"
  },
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node index.js",
    "check-heic": "node -e \"console.log('HEIC support check:'); const sharp = require('sharp'); console.log('Sharp version:', sharp.versions.sharp); console.log('HEIF support:', !!sharp.format.heif?.input);\""
  },
  "keywords": [
    "image",
    "cli",
    "manipulation",
    "resize",
    "convert",
    "watermark",
    "heic",
    "heif",
    "batch",
    "meme",
    "collage",
    "filters",
    "sharp",
    "ios",
    "macos",
    "photo",
    "processing",
    "terminal"
  ],
  "author": "M. Bilal Khan <intelligencexempire@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/realBilalKhan/pixpress.git"
  },
  "bugs": {
    "url": "https://github.com/realBilalKhan/pixpress/issues"
  },
  "homepage": "https://github.com/realBilalKhan/pixpress#readme",
  "type": "module",
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "dependencies": {
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "figlet": "^1.9.3",
    "fs-extra": "^11.3.2",
    "gradient-string": "^3.0.0",
    "inquirer": "^12.9.6",
    "minimatch": "^9.0.5",
    "ora": "^7.0.1",
    "sharp": "^0.34.4"
  },
  "optionalDependencies": {
    "heic-convert": "^2.1.0"
  }
}
